        </xs:annotation>
      </xs:element>
      <xs:element name="HasValue" type="xs:double" maxOccurs="1" minOccurs="1">
        <xs:annotation>
          <xs:documentation xml:lang="en" source="#definition">A numeric value indicating the amount of the Cost.	</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
